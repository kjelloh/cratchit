{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Attach",
      "type": "cppdbg",
      "request": "attach",
      "program": "${workspaceFolder}/cratchit.out",
      "processId": "${command:pickProcess}",
      "MIMode": "lldb",
      "preLaunchTask": "macOS C/C++: g++-11 build cratchit",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "g++-11 - Build and debug cratchit",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cratchit.out",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb",
      "preLaunchTask": "macOS C/C++: g++-11 build cratchit"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        ""
      ],
      "stopAtEntry": true,
      "cwd": "/Users/kjell-olovhogdal/Documents/Github/cratchit",
      "environment": [],
      "program": "/Users/kjell-olovhogdal/Documents/Github/cratchit/build/Debug/outDebug",
      "internalConsoleOptions": "openOnSessionStart",
      "MIMode": "gdb",
      "externalConsole": true
    }
  ]
